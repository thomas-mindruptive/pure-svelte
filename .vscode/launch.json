{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Generate Images",
        "program": "${workspaceFolder}/tools/dist/tools/images/genImagest.js",
        "cwd": "${workspaceFolder}",
        "envFile": "${workspaceFolder}/.env",
        "sourceMaps": true,
        "skipFiles": [
          "<node_internals>/**"
        ],
        "console": "integratedTerminal",
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ]
      },
    ]
  }